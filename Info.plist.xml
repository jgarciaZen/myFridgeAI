<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- App Name -->
    <key>CFBundleName</key>
    <string>Grocery & Meal Planner</string>
    
    <!-- Display Name (shown on home screen) -->
    <key>CFBundleDisplayName</key>
    <string>GroceryMealPlanner</string>
    
    <!-- Unique identifier for your app -->
    <key>CFBundleIdentifier</key>
    <string>com.yourcompany.GroceryMealPlanner</string>
    
    <!-- App version -->
    <key>CFBundleShortVersionString</key>
    <string>1.0</string>
    
    <!-- Build number -->
    <key>CFBundleVersion</key>
    <string>1</string>
    
    <!-- Specify that this app requires iOS -->
    <key>LSRequiresIPhoneOS</key>
    <true/>
    
    <!-- Launch screen file -->
    <key>UILaunchStoryboardName</key>
    <string>LaunchScreen</string>
    
    <!-- Main storyboard file (if you use storyboards) -->
    <key>UIMainStoryboardFile</key>
    <string>Main</string>
    
    <!-- Permissions and usage descriptions -->
    <key>NSCameraUsageDescription</key>
    <string>This app requires camera access to scan grocery receipts.</string>
    <key>NSPhotoLibraryUsageDescription</key>
    <string>This app requires access to your photo library to select receipts.</string>
    
    <!-- Other keys can be added as needed -->
</dict>
</plist>
